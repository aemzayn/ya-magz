{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/assets/*": ["src/assets/*"],
      "@/animations/*": ["src/components/animations/*"],
      "@/lib/*": ["src/lib/*"],
      "@/styles/*": ["styles/*"],
      "@/cms/*": ["cms/*"],
      "@/constants/*": ["src/constants/*"],
      "@/public/*": ["public/*"]
    }
  },
  "exclude": ["node_modules"]
}
